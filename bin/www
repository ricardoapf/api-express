#!/usr/bin/env node

/**
 * Module dependencies.
 */

let app = require('../app');
let http = require('http');

let port = process.env.PORT || 3000;
let server = http.createServer(app);
app.set('port', port);

/**
 * Listen on provided port, on all network interfaces.
 */

server.listen(port);
server.on('listening', function () {
  console.log('Listening on ' + port);
});